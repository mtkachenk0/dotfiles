call plug#begin('~/.vim/plugged')

Plug 'morhetz/gruvbox'        " gruvbox colorscheme

"Fugitive Vim Github Wrapper
Plug 'tpope/vim-fugitive'
" Finders
Plug 'junegunn/fzf', { 'do': { -> fzf#install() } }
Plug 'junegunn/fzf.vim'

Plug 'tpope/vim-unimpaired'   " awesome pair mappings
Plug 'tpope/vim-endwise'      " auto-add end statements in the code
Plug 'tomtom/tcomment_vim'    " Comment code
Plug 'airblade/vim-gitgutter' " git diff visualizer
Plug 'godlygeek/tabular'		  " awesome!!! alignments
Plug 'foosoft/vim-argwrap'
