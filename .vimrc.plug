Plug 'tpope/vim-rails'        " adds easy way to regenerate tags
Plug 'tpope/vim-rbenv'        " provides path support for rbenv ruby files
Plug 'christoomey/vim-tmux-navigator'
Plug 'maxmellon/vim-jsx-pretty'
